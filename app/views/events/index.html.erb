<h1>Listing events</h1>

<table>
    <tr>
        <th>Event</th>
        <th>Where</th>
        <th>Google_id</th>
        <th>Start_time</th>
        <th>End_time</th>
        <th></th>
    </tr>

<% @events_from_google.each do |event| %>
    <tr>
        <td><%= event.title %></td>
        <td><%= event.where %></td>
        <td><%= event.google_id %></td>
        <td><%= event.start_time %></td>
        <td><%= event.end_time %></td>
        <td><%= link_to 'Show', event %></td>
    </tr>
<% end %>
</table>